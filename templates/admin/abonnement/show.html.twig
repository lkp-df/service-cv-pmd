{% extends 'base.html.twig' %}

{% block title %}Abonnement{% endblock %}

{% block body %}
    <h1>Abonnement</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ abonnement.id }}</td>
            </tr>
            <tr>
                <th>Debut</th>
                <td>{{ abonnement.debut ? abonnement.debut|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Fin</th>
                <td>{{ abonnement.fin ? abonnement.fin|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{{ abonnement.statut }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_abonnement_index') }}">back to list</a>

    <a href="{{ path('admin_abonnement_edit', {'id': abonnement.id}) }}">edit</a>

    {{ include('admin_abonnement/_delete_form.html.twig') }}
{% endblock %}
