{% extends "admin/base.html.twig" %}
{% import "macros/link.html.twig" as link %}
{% import "macros/breadcrumb.html.twig" as breadcrumb %}

{% block title %}New Commande{% endblock %}
 
{% block breadcrumb %}
    {{ breadcrumb.breadcrumb('Create new Commande'|trans,[

        {
            name:'Listes des commandes',
            path:path('admin_commande_index')
        }
    ]) }}
 {% endblock %}

{% block body %}
<div class="card card-outline card-primary">
    <div class="card-header">
        <div class="card-title">
            {{'Create new Commande'|trans}}
        </div>
    </div>
    <div class="card-body">
        {{ include('admin/commande/_form.html.twig') }}
        <hr>
        {{ link.back(path('admin_commande_index')) }}
    </div>
</div>
{% endblock %}
