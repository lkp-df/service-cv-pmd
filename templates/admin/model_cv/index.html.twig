{% extends 'admin/base.html.twig' %} {% import "macros/link.html.twig" as link %} {% import "macros/dataTable.html.twig" as dataTable %} {% import "macros/alertes.html.twig" as alert %} {% block title %}Modele CV {% endblock %} {% block head %} {{ dataTable.head()
}} {% endblock %} {% block foot %} {{ dataTable.js() }} {% endblock %} {% block javascripts %} {{ dataTable.script('#table-1') }} {% endblock %} {% block body %}


<div class="card card-outline card-blue">
    <div class="card-header">
        <h1>Create CV Modele</h1>
    </div>
    <div class="card-body">
        <div class="row">
            <div class="col">
                <h3>
                    {{ link.add(path('model_new'),'Create new') }}
                </h3>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <table class="table" id="table-1">
                    <thead>
                        <tr>
                            <th>Designation</th>
                            <th>Prix</th>
                            <th>Image</th>
                            <th>Created_at</th>
                            <th>actions</th>
                        </tr>
                    </thead>

                    <tbody>
                        {% for model_cv in model_cvs %}
                        <tr>
                            <td>{{ model_cv.designation }}</td>
                            <td>{{ model_cv.prix }}</td>
                            <td>{{ model_cv.image }}</td>
                            <td>{{ model_cv.createdAt ? model_cv.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                            <td>
                                <a href="{{ path('model_show', {'id': model_cv.id}) }}">show</a>
                                <a href="{{ path('model_edit', {'id': model_cv.id}) }}">edit</a>
                            </td>
                        </tr>
                        {% else %}
                        <tr>
                            <td colspan="6">no records found</td>
                        </tr>
                        {% endfor %}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
{% endblock %}