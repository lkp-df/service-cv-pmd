{% extends 'admin/base.html.twig' %}
{% import "macros/link.html.twig" as link %}
{% import "macros/button.html.twig" as btn %}
{% block title %}Type Abonnement
{% endblock %}
{% block body %}

	<div class="card card-outline card-success">
		<div class="card-header">
			<h3>Edit Type Abonnement</h3>
		</div>
		{{form_start(form)}}
		<div class="card-body">
			<div class="row">
				<div class="col-md-12 col-lg-12">
					{{form_row(form.designation)}}
					{{form_row(form.montant)}}
					{{form_row(form.nombreCv)}}
				</div>
			</div>
			<div class="row">
				<div class="col">
					<div class="btn-group">
						{{link.back(path('admin_type_abonnement_index'))}}
						{{btn.update()}}
					</div>
				</div>
			</div>
		</div>
		{{form_end(form)}}
	</div>
{% endblock %}
